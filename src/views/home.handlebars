    <h1>Home</h1>

    <div id="productsContainer">
        {{#each products}}
            <div class="card">
                <p>{{this.title}}</p>
                <p>{{this.description}}</p>
                <p>{{this.price}}</p>
                <p>{{this.thumbnail}}</p>
                <p>{{this.code}}</p>
                <p>{{this.stock}}</p>
                <p>{{this.id}}</p>
                <p>{{this.category}}</p>
            </div>
        {{/each}}
    </div>


<script src="https://cdn.socket.io/4.7.4/socket.io.min.js"></script>
<script src="js/main.js"></script>